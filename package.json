{
  "name": "react-frontend-app",
  "version": "1.0.0",
  "description": "Frontend App with React and Typescript",
  "main": "index.tsx",
  "scripts": {
    "dev:live": "webpack server --mode=development --env TARGET=development",
    "build": "webpack --mode=production --progress",
    "build:production": "webpack --mode=production --progress --env TARGET=production",
    "prettier": "prettier 'src/**/*.{ts,tsx}'",
    "prettier:check": "npm run prettier -- --check",
    "prettier:format": "npm run prettier -- --write",
    "type-check": "echo \"type-checking...\" && tsc --noEmit --skipLibCheck",
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "lint": "eslint src --ext .js,.ts,.tsx,.jsx",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "npm run lint -- --max-warnings=2"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jesscorda/react-frontend-app.git"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@eslint/js": "^9.4.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.2.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.12.0",
    "autoprefixer": "^10.4.19",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "css-loader": "^7.1.2",
    "eslint": "^8.57.0",
    "eslint-config-love": "^52.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^28.6.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-webpack-plugin": "^4.2.0",
    "globals": "^15.4.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.4.38",
    "postcss-loader": "^8.1.1",
    "prettier": "3.3.1",
    "react-test-renderer": "^18.3.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^3.4.4",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.12.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "core-js": "^3.37.1",
    "husky": "^9.0.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1"
  }
}
